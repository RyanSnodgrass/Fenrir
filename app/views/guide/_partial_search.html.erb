<% if results.size == 0 %>
  <% if params[:page] == 1 %>
    <div class="search_results_msg">No results match your current criteria</div>
    <% end %>
<% else %>
  <% results.each do |result| %>
    <%= render partial: "#{result['type'].pluralize}/partial_search", locals: { result: result }, layout: false %>
   <%end%>

   <div class="more_results" id="<%= Time.now.getutc.to_s%>" foo="<%= params[:page].to_i + 1 %>" data-next-page="<%= params[:page].to_i + 1 %>">More Results</div>
<% end %>
