.row
  .result_row
    .result_header
      .result_title
        %h4
          %a.do_highlight{:href => "../#{result["&type"].pluralize}/#{result["name"]}"}
            = result["name"]
    / .shape
    /   %p= result["type"]
    .do_highlight.result_body
      - if result["definition"]  then
        = result["definition"].html_safe
%hr